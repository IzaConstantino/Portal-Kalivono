// Page Content
block content
    .row
        - if (termos != null)
            each termo in termos
                .col-sm-4.col-lg-4.col-md-4
                    .thumbnail  
                        if termo.multimidia
                            img(src=termo.multimidia[0].url, alt=' ')
                            .caption
                                h4.pull-right
                                h4
                                    //- a(href='# ') Artesanato
                                    p.text-center=termo.emPortugues
                                    p.text-center=termo.emTerena
                                    p.text-center=termo.categoria
                                p
                                    //- a(target='_blank ', href='http://www.bootsnipp.com ') Bootsnipp - http://bootsnipp.com
                            .ratings
                                p.pull-right Leia mais
                                p
                                    span.glyphicon.glyphicon-star
                                    span.glyphicon.glyphicon-star
                                    span.glyphicon.glyphicon-star
                                    span.glyphicon.glyphicon-star
                                    span.glyphicon.glyphicon-star
